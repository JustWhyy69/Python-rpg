class Character():
    def _init_(self):  
        self.name = "Natilya"
        self.strength = 11
        self.intelligence = 15
        self.wisdom = 14
        self.dexterity = 15 
        self.constitution = 14 
        self.charisma = 16
        self.race = "Nymph"
        self.clas = "Druid"
        self.level = 0
        self.max_hp = 0
        self.movement = 30
        self.armors =""
        self.weapons = ""
        self.xp =0
        self.type ="" 
        self.current_weapon = "fist"
        self.current_armor = ""